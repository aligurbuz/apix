#!/usr/bin/env php
<?php

$commandFile=require("./lib/bin/commands/".$argv[1].".php");
$commandFile=new $argv[1]();

$list=[];
foreach ($argv as $key=>$value){
    if($key>2){

        if(preg_match('@:@is',$value))
        {
            $value=explode(":",$value);
            $list[$value[0]]=$value[1];
        }
        else
        {
            $list[$value]=$value;
        }

    }
}


echo $commandFile->$argv[2]($list);
echo '
';
